generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model inv-festivitie{
  id              int      @id @unique @default(autoincrement())
  title           string   
  description     string?
  date            string
  origenCountry   string 
} 